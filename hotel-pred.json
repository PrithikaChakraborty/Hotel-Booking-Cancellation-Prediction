{
  "family": "hotel-pred",
  "networkMode": "bridge",
  "executionRoleArn": "arn:aws:iam::289997608587:role/ecsTaskexecutor",
  "taskRoleArn": "arn:aws:iam::289997608587:role/ecsTaskexecutor",
  "requiresCompatibilities": [
    "EC2"
  ],
  "containerDefinitions": [
    {
      "name": "hotel-pred",
      "image": "placeholder-image",
      "cpu": 0,
      "memoryReservation": 512,
      "essential": true,
      "portMappings": [
        {
          "containerPort": 5000,
          "hostPort": 0,
          "protocol": "tcp"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/hotel-pred",
          "awslogs-create-group": "true",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ]
}

