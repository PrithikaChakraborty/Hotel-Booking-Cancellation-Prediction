{
  "family": "hotel-pred",
  "networkMode": "awsvpc",
  "executionRoleArn": "arn:aws:iam::289997608587:role/ecsTaskexecutor",
  "taskRoleArn": "arn:aws:iam::289997608587:role/ecsTaskexecutor",
  "cpu": "1024",
  "memory": "3072",
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "containerDefinitions": [
    {
      "name": "hotel-pred",
      "image": "placeholder-image",
      "cpu": 1024,
      "essential": true,
      "portMappings": [
        {
          "containerPort": 5000,
          "protocol": "tcp"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/hotel-pred",
          "awslogs-create-group": "true",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "ecs"
        }
      },
      "memory": 2048
    }
  ]
}
